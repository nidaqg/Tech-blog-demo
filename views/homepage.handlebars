<section class="col-sm-6 justify-content-center text-center">
{{#each blogposts as |blogpost|}}
<section class="row">
  <section class="card m-2 localCard"
    <section class="card-body">
      <h5 class="text-center">{{blogpost.post_title}}</h5>
      <h4 class="text-center">{{blogpost.date_created}}</h4>
        <p>{{blogpost.content}}</p>
    </section>
  </section>
</section>
{{/each}}
</section>